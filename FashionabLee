#include <bits/stdc++.h>

using namespace std;


void solve() {
     int n;cin>>n;
     
     // if a polygon divided by 4 it is create a 90 degree angle.
     if(n%4==0){cout<<"YES"<<endl;}
     else
     {
         cout<<"NO"<<endl;
     }
}
int main(){
    ios_base::sync_with_stdio(false);
    cin.tie(nullptr);
    int t;
    cin >> t;
    while (t--) {
        solve();
    }
}
